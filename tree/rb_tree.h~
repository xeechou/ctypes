#define RED   1
#define BLACK 0
/* file of black red tree implementation */
typedef struct BRnode {
	int color;
	Telemtype data;
	struct BRnode *lsubtr;
	struct BRnode *rsubtr;
	struct BRnode *parent;
}*brnode;

extern int left_rotate(brnode root, brnode x);

extern int right_rotate(brnode root, brnode x);

extern int rb_insert(brnode root, Telemtype entry);

extern int rb_insert_fix(brnode root, brnode z);

/* simple toolkit */
extern brnode grandp(brnode);
extern brnode uncle(brnode);
extern int color(brnode);
